{
  "author": "Michael D. Norman",
  "name": "tutorial-openzeppelin-contract",
  "description": "",
  "private": true,
  "license": "commercial",
  "version": "0.0.0",
  "homepage": "https://github.com/mdnorman/CHANGEME/wiki/Home",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdnorman/CHANGEME"
  },
  "bugs": {
    "url": "https://github.com/mdnorman/CHANGEME/issues"
  },
  "scripts": {
    "clean": "rm -rf build/contracts && rm -rf dist && rm -rf out",
    "compile:contracts": "oz compile && npm run pretty && npm run lint",
    "build": "npm run compile:contracts && tsc",
    "build:watch": "tsc --watch",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "posttest": "npm run lint",
    "lint": "solhint contracts/**/*.sol && tslint -c tslint.json --project tsconfig.json && tslint -c tslint.json --project spec/tsconfig.json",
    "pretty": "pretty-quick --stage",
    "network:development": "ganache-cli --deterministic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty",
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@openzeppelin/contract-loader": "0.6.2",
    "@openzeppelin/contracts-ethereum-package": "2.5.0",
    "@openzeppelin/upgrades": "2.8.0",
    "web3": "1.3.6",
    "web3-core": "1.3.6"
  },
  "devDependencies": {
    "@openzeppelin/cli": "2.8.2",
    "@openzeppelin/test-environment": "0.1.9",
    "@openzeppelin/test-helpers": "0.5.11",
    "@truffle/hdwallet-provider": "1.4.1",
    "@types/bn.js": "4.11.6",
    "@types/jest": "25.2.3",
    "@types/node": "13.7.4",
    "ganache-cli": "6.12.2",
    "husky": "4.3.8",
    "jest": "26.0.0",
    "jest-config": "25.3.0",
    "mnemonics": "1.1.3",
    "prettier": "2.3.2",
    "prettier-plugin-solidity": "1.0.0-beta.13",
    "pretty-quick": "2.0.2",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.2",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.2.0",
    "tsutils": "3.21.0",
    "typescript": "3.9.10"
  },
  "optionalDependencies": {},
  "postinstall": "rm node_modules/web3/types/index.d.ts",
  "engines": {
    "npm": ">=6.13",
    "node": ">=12.16"
  }
}
